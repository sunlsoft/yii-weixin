{
	"name" : "sunlsoft/yii-weixin",
	"description" : "wechat extension for the Yii framework",
	"keywords" : [
		"yii2",
		"wechat"
	],
	"type" : "yii2-extension",
	"license" : "BSD-3-Clause",
	"support" : {
		"forum" : "http://www.yiiframework.com/forum/",
		"wiki" : "http://www.yiiframework.com/wiki/",
		"irc" : "irc://irc.freenode.net/yii",
		"source" : "https://github.com/sunlsoft/yii-weixin"
	},
	"authors" : [{
			"name" : "sun",
			"email" : "sunhongen1992@163.com"
		}
	],
	"require" : {
		"php" : ">=5.4.0",
		"ext-mcrypt" : "*",
		"yiisoft/yii2" : "~2.0.13",
		"yiisoft/yii2-httpclient" : "~2.0.0"
	},
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}
	],
	"autoload" : {
		"psr-4" : {
			"sunlsoft\\yiiweixin\\" : "src"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.0.x-dev"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^6.2"
	}
}
